#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#include <cctype>
using namespace std;
bool jeprost(int& broj) {
	int brojdjelitelja = 0;
	if (broj < 2) {
		return false;
	}
	for (int j = 1; j <= broj; j++) {
		if (broj % j == 0) {
			brojdjelitelja += 1;
		}
	}
	return brojdjelitelja == 2;
	
}
void funkcija(vector<int> &v) {
	int n = 2;
	int k = 2;
	vector<int> novi(v.size());
	vector<int> novi1;
	/*transform(v.begin(), v.end(), novi.begin(), [&](int broj) {
