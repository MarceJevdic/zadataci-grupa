#include <iostream>
using namespace std;
bool uzlazno(int a, int b) {
	if (a < b){
		return true;
	}
	return false;

}
bool silazno(int a, int b) {
	if (a > b) {
		return true;
	}
	return false;
}

void sortt(int niz[], int n, bool (*usporedi)(int a, int b)) {
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n - 1 - i; j++) {
			if (!usporedi(niz[j],niz[j+1])) {
				int tmp = niz[j];
				niz[j] = niz[j + 1];
				niz[j + 1] = tmp;
			}
		}
	}
}

int main() {
	int niz[50];
	int n;
	cout << "unesi broj elemenata niza: " << endl;
	cin >> n;
	cin.ignore();
	cout << "unesi elemente niza:" << endl;
	for (int i = 0; i < n; i++) {
		cin >> niz[i];
		cin.ignore();
	}

	sortt(niz, n, uzlazno);
	for (int i = 0; i < n; i++) {
		cout << niz[i]<<" ";
	}

	return 0;
}
