#include <iostream>
#include <algorithm>
#include <vector>

using namespace std;
template <typename T>
void funkcija(vector<T>& v1) {
	for (size_t i= 0; i < v1.size(); i += 2) {
		v1.insert(v1.begin()+i + 1, v1[i]);
	}
	
}
int main() {
	vector<string> brojevi = {"ja","sam","maka"};
	funkcija(brojevi);
	for (auto x : brojevi) {
		cout << x;
	}
	return 0;
}
