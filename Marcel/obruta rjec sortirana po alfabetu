#include <iostream>
#include <string>
#include<vector>
#include <algorithm>
using namespace std;

void funkcija(string& rjec) {
    auto pocetak = rjec.begin();
    vector<string> rijeci;
    while (pocetak != rjec.end()) {
        auto it = find(pocetak, rjec.end(), ' ');


        string rijec(pocetak, it);
        sort(rijec.begin(), rijec.end());
        rijeci.push_back(rijec);
        if (it == rjec.end()) {
            break;
        }
        pocetak = it +1;
    }
    


    reverse(rijeci.begin(), rijeci.end());

    for (size_t i = 0; i < rijeci.size(); ++i) {
        cout << rijeci[i];
        if (i != rijeci.size() - 1) cout << ' ';
    }
    cout << endl;

}

int main() {
    string rjec = "better late than never";
    funkcija(rjec);
    return 0;
}
